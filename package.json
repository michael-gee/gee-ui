{
  "name": "@gee-ui/react",
  "version": "1.0.0-beta.3",
  "description": "Single source of truth react component library for all things shadcn/ui.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    },
    "./tailwind-preset": {
      "require": "./dist/tailwind-preset.cjs",
      "types": "./dist/tailwind-preset.d.ts"
    }
  },
  "scripts": {
    "build": "export NODE_OPTIONS=--max-old-space-size=8192 && tsup --external pkgJson",
    "deps:check": "npx npm-check-updates",
    "deps:upgrade": "npx npm-check-updates -i",
    "dev": "storybook dev -p 6006",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "embla-carousel-react": "^8.1.8",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.378.0",
    "react-day-picker": "^9.0.8",
    "react-resizable-panels": "^2.0.20",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.7.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-mdx-gfm": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/manager-api": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/theming": "^8.2.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.41",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.9",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "keywords": [
    "shadcn",
    "react",
    "tailwind",
    "radix",
    "ui",
    "components"
  ],
  "bugs": {
    "url": "https://github.com/michael-gee/gee-ui/issues"
  },
  "author": "Michael Gee",
  "license": "MIT"
}
