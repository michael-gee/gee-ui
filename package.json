{
  "name": "gee-ui",
  "version": "0.6.0",
  "description": "Single source of truth react component library for all things shadcn/ui.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "storybook dev -p 6006",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "clsx": "^2.1.1",
    "cva": "npm:class-variance-authority@^0.7.0",
    "lucide-react": "^0.378.0",
    "tailwind-merge": "^2.3.0"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x",
    "tailwindcss": "3.x"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/react-vite": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.1",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michael-gee/gee-ui.git"
  },
  "bugs": "https://github.com/michael-gee/gee-ui/issues",
  "author": "Michael Gee",
  "license": "MIT"
}
